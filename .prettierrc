{
  "arrowParens": "always",
  "bracketSpacing": true,
  "semi": false,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "overrides": [
    {
      "files": ["*.json", ".prettierrc", ".babelrc"],
      "options": {"parser": "json"}
    }
  ]
}
